{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    },
    {
        "src": "static/**",
        "use": "@vercel/static"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"
    }],
    "routes": [{
        "src": "/static/(.*)",
        "dest": "/static/(.*)"
    },{
        "src": "/(.*)",
        "dest": "/"
    }]
}